{
    "name": "atorscho/membership",
    "description": "Powerful user membership system for Laravel 5 with user groups and permissions.",
    "keywords": [
        "membership",
        "users",
        "groups",
        "permissions",
        "roles",
        "capabilities",
        "abilities",
        "laravel"
    ],
    "homepage": "https://alextorscho.com/docs/user-membership",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Torscho",
            "email": "contact@alextorscho.com",
            "homepage": "https://alextorscho.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1",
        "illuminate/support": "5.4.* - 5.5.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "laravel/laravel": "5.4.*"
    },
    "autoload": {
        "files": [
            "helpers/membership.php"
        ],
        "psr-4": {
            "Atorscho\\Membership\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atorscho\\Membership\\Tests\\": "tests/"
        },
        "classmap": [
            "migrations"
        ]
    }
